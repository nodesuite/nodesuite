{
  "$schema": "https://json.schemastore.org/package",
  "name": "@nodesuite/case",
  "version": "0.1.3",
  "description": "Typed string and object key case transformers.",
  "keywords": [
    "case",
    "change-case",
    "key-case",
    "nodesuite"
  ],
  "homepage": "https://github.com/nodesuite/nodesuite",
  "bugs": "https://github.com/nodesuite/nodesuite/issues",
  "repository": "https://github.com/nodesuite/nodesuite",
  "license": "ISC",
  "author": "@nodesuite",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "barrels": "run-s barrels:*",
    "barrels:generate": "barrelsby -d src -c node_modules/@nodesuite/node-rig/profiles/default/barrelsby.json",
    "barrels:lint": "eslint src/**/index.ts --fix",
    "build": "heft build --clean",
    "build:prod": "tsup-node --config=node_modules/@nodesuite/node-rig/profiles/default/tsup.config.js",
    "circular": "dpdm --ext ts --circular --output .rush/circular.json src ",
    "clean": "run-p clean:*",
    "clean:heft": "heft clean --clear-cache",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint src --ext .ts --fix",
    "lint:sort": "sort-package-json",
    "typecheck": "tsc --noEmit",
    "upgrade": "ncu -u --packageFile package.json"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "tslib": "^2.4.0",
    "type-fest": "^2.19.0"
  },
  "devDependencies": {
    "@nodesuite/eslint-config": "workspace:*",
    "@nodesuite/node-rig": "workspace:*",
    "@rushstack/eslint-config": "^3.0.1",
    "@rushstack/heft": "^0.47.9",
    "@types/node": "^18.7.18",
    "@typescript-eslint/parser": "^5.37.0",
    "ava": "^4.3.3",
    "barrelsby": "^2.4.0",
    "c8": "^7.12.0",
    "dpdm": "^3.10.0",
    "eslint": "8.22.0",
    "npm-check-updates": "^15.3.4",
    "npm-run-all": "^4.1.5",
    "sort-package-json": "^1.57.0",
    "tsup": "^6.2.3",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
