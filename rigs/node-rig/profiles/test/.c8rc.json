{
  "$schema": "https://json.schemastore.org/nycrc",
  "all": true,
  "check-coverage": true,
  "clean": true,
  "reporter": ["lcov", "text"],
  "src": "src",
  "exclude": ["src/types/*.ts", "src/index.ts"],
  "include": ["src/**/*.ts"],
  "report-dir": "coverage",
  "temp-dir": "temp",
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
}
