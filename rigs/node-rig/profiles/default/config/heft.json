{
  "$schema": "https://developer.microsoft.com/json-schemas/heft/heft.schema.json",
  "eventActions": [
    {
      "actionKind": "deleteGlobs",
      "heftEvent": "clean",
      "actionId": "defaultClean",
      "globsToDelete": [
        "src/**/*.js*",
        "*.build.log",
        "*.build.error.log",
        "temp/",
        "dist/",
        "lib/"
      ]
    }
  ],
  "heftPlugins": [
    {
      "plugin": "@rushstack/heft-jest-plugin",
      "options": {}
    }
  ]
}
